;; MODES
;; +1 enable
;; -1 disable

;; Disable menu-bar
(menu-bar-mode -1)

;; Disable tool-bar
(tool-bar-mode -1)

;; Disable scroll-bar
(scroll-bar-mode -1)

;; Enable line-number
(global-display-line-numbers-mode +1)

;; Enable company mode
(global-company-mode +1)

;; Enable auto-complete
(global-auto-complete-mode +1)
